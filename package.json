{
  "name": "my-buddy-gymmie",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/react-native": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-each": "^0.3.1",
    "jest-expo": "26.0.0",
    "react-dom": "^16.3.3",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --maxWorkers=4",
    "devtest": "node node_modules/jest/bin/jest.js --coverage --watch",
    "lint": "./node_modules/.bin/eslint .",
    "storybook": "concurrently --kill-others \"storybook start -p 7007\" \"REACT_NATIVE_STORYBOOK=true react-native-scripts start\""
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!react|@expo|expo|react-router-native)"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./src/setupEnzyme.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "expo": "^26.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "moment": "^2.29.4",
    "prop-types": "^15.6.1",
    "react": "16.3.0-alpha.1",
    "react-apollo": "^2.1.3",
    "react-native": "0.54.0",
    "react-native-elements": "^0.19.1",
    "react-redux": "^5.0.7",
    "react-router-native": "^4.2.0",
    "react-router-native-stack": "^0.0.11",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0"
  }
}
